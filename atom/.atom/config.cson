"*":
  "autoclose-html":
    makeNeverCloseSelfClosing: false
  core:
    disabledPackages: [
      "language-clojure"
      "language-go"
      "language-mustache"
      "ide-csharp"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      ".sass-cache"
    ]
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "base16-syntax"
    ]
    titleBar: "custom"
  editor:
    preferredLineLength: 120
    scrollPastEnd: true
    showIndentGuide: true
    tabLength: 4
  "exception-reporting":
    userId: "c6ecdfe6-c496-4331-937d-5bb16acf637e"
  "markdown-preview": {}
  minimap:
    displayCodeHighlights: false
  "tree-view":
    hideIgnoredNames: true
    squashDirectoryNames: true
  "vim-mode-plus":
    blackholeRegisteredOperators: [
      "delete*"
      "substitute*"
    ]
    keymapYToYankToLastCharacterOfLine: true
  welcome:
    showOnStartup: false
".basic.html.text":
  editor:
    softWrap: true
".css.scss.source":
  editor:
    softWrapHangingIndent: 2
".css.source":
  editor:
    softWrapHangingIndent: 2
    tabType: "soft"
".sass.source":
  editor:
    softWrapHangingIndent: 2
    tabType: "soft"
".source.yaml":
  editor:
    autoIndentOnPaste: false
    tabLength: 2
